{
  "name": "tutor-support-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "api": "tsx server.ts",
    "dev:api": "tsx watch server.ts",
    "ws": "tsx ws-server.ts",
    "dev:ws": "tsx watch ws-server.ts",
    "test:api": "tsx scripts/testAPIs.ts",
    "test:messages": "tsx scripts/testMessages.ts",
    "test:online-status": "tsx scripts/test-online-status.ts",
    "create:test-accounts": "tsx scripts/createTestAccounts.ts",
    "build": "tsc && npx vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "seed": "tsx lib/seed.ts",
    "seed:clean": "node -e \"require('fs').rmSync('data', {recursive: true, force: true})\" && npm run seed",
    "validate": "tsx scripts/validate.ts",
    "stats": "tsx -e \"import('./lib/dataValidator').then(m => m.generateDataStats())\"",
    "upload:blob": "tsx scripts/upload-to-blob.ts",
    "check:blob": "tsx scripts/check-blob-storage.ts",
    "test:blob": "tsx scripts/test-blob-token.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@gsap/react": "^2.1.2",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-charts": "^8.14.0",
    "@mui/x-date-pickers": "^8.14.1",
    "@mui/x-date-pickers-pro": "^8.14.1",
    "@vercel/blob": "^2.0.0",
    "@vercel/node": "^5.5.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "gsap": "^3.13.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "nanoid": "^5.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.2.3",
    "react-router-dom": "^6.20.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/jsonwebtoken": "^9.0.10",
